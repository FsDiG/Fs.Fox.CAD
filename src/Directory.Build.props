<Project>
    <!--版本号和提示-->
    <PropertyGroup>
        <Version>0.9.2-preview6</Version>
        <PackageReleaseNotes>优化点在多边形内的判断逻辑</PackageReleaseNotes>
    </PropertyGroup>

    <PropertyGroup>
        <LangVersion>preview</LangVersion>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <EnableNETAnalyzers>true</EnableNETAnalyzers>
        <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
        <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
        <GenerateDocumentationFile>True</GenerateDocumentationFile>
        <PlatformTarget>x64</PlatformTarget>
        <RootNamespace>Fs.Fox.CAD</RootNamespace>
        <UseWindowsForms>true</UseWindowsForms>
        <UseWpf>true</UseWpf>
        <OutputPath>..\..\bin\$(Configuration)\</OutputPath>
    </PropertyGroup>

    <PropertyGroup><!--nuget包信息-->
        <Company>FS Team</Company>
        <Authors>xsfhlzh;vicwjb;liuqihong;DYH;FS Team</Authors>
        <Copyright>FS Team</Copyright>
        <Description>基于.NET的CAD二次开发基础类库，支持AutoCAD和中望CAD多个版本。提供完整的API封装，简化CAD插件开发。源于IFoxCAD项目。</Description>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <PackageProjectUrl>https://github.com/FsDiG/Fs.Fox.CAD</PackageProjectUrl>
        <RepositoryUrl>https://github.com/FsDiG/Fs.Fox.CAD.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>IFox;Fs.Fox;CAD;AutoCAD;ZWCAD;中望CAD;二次开发;Plugin;C#;.NET;ARX</PackageTags>
        <PackageReadmeFile>readme.md</PackageReadmeFile>
        <PackageLicenseFile>LICENSE</PackageLicenseFile>
        <PackageIcon>icon.png</PackageIcon>
    </PropertyGroup>

</Project>